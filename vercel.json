{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "pantheon",
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ],
  "env": {
    "PYTHONPATH": "/var/task:/var/task/physics_swarm",
    "TAVILY_API_KEY": "demo_key"
  },
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "ignoreCommand": "node vercel-build-check.js"
} 