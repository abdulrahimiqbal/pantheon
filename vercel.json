{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "pantheon",
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    },
    "api/test.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "ignoreCommand": "node vercel-build-check.js"
} 