{
  "version": 2,
  "name": "pantheon",
  "builds": [
    {
      "src": "orchestration/main.py",
      "use": "@vercel/python@4.1.0",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/orchestration/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/orchestration/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "orchestration/main.py": {
      "runtime": "python3.11"
    }
  },
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "ignoreCommand": "node vercel-build-check.js"
} 